{
  "name": "app_package",
  "description": "Terraform package for creating an application that depends on network",
  "version": "0.4.0",
  "native": {
    "type": "terraform",
    "path": "."
  },
  "props": {
    "name": {
      "description": "Name of the application",
      "type": "string"
    },
    "network_id": {
      "default": "",
      "description": "ID of the VPC from network package (optional for testing)",
      "type": "string"
    },
    "port": {
      "default": 8080,
      "description": "Port for the application",
      "type": "number"
    }
  },
  "outs": {
    "app_id": {
      "value": "id",
      "type": "string",
      "description": "ID of the created application resource"
    },
    "app_name": {
      "value": "name",
      "type": "string",
      "description": "Name of the application"
    }
  },
  "state": "managed",
  "tags": []
}