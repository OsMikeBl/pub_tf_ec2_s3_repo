{
  "name": "network_package",
  "description": "Terraform package for creating a network",
  "version": "0.1.0",
  "native": {
    "type": "terraform",
    "path": "."
  },
  "props": {
    "name": {
      "type": "string",
      "description": "Name of the network/VPC"
    },
    "cidr": {
      "type": "string",
      "description": "CIDR block for the network",
      "default": "10.0.0.0/16"
    }
  },
  "outs": {
    "vpc_id": {
      "type": "string",
      "description": "ID of the created VPC",
      "value": "id"
    },
    "vpc_cidr": {
      "type": "string",
      "description": "CIDR block of the VPC",
      "value": "cidr_block"
    }
  },
  "state": "managed",
  "tags": []
}